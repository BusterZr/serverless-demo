{
	"name": "ciam-node-sdk",
	"version": "1.1.1",
	"main": "dist/index.js",
	"typings": "dist/index.d.ts",
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"start": "tsdx watch",
		"build": "npx tsdx build",
		"test": "tsdx test --passWithNoTests",
		"size": "size-limit",
		"analyze": "size-limit --why",
		"deploy": "node ../../scripts/publish.js"
	},
	"devDependencies": {
		"@types/react": "^17.0.14",
		"react": "^17.0.2",
		"react-docgen-typescript-loader": "^3.7.2",
		"regenerator-runtime": "^0.13.9"
	},
	"dependencies": {
		"ciam-js-sdk": "^1.1.2",
		"crypto": "^1.0.1"
	},
	"peerDependencies": {
		"react": ">=16.8.0"
	},
	"prettier": {
		"printWidth": 80,
		"semi": true,
		"singleQuote": true,
		"trailingComma": "es5"
	},
	"author": "shingao",
	"module": "dist/ciam-spa-sdk.esm.js",
	"size-limit": [
		{
			"path": "dist/ciam-spa-sdk.cjs.production.min.js",
			"limit": "10 KB"
		},
		{
			"path": "dist/ciam-spa-sdk.esm.js",
			"limit": "10 KB"
		}
	]
}
